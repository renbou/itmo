tokens {
  ident = "[a-zA-Z_][a-zA-Z0-9_]*";
}

rules {
  LambdaDecl = "lambda" Args ":" Expr;

  Args    = ident OptArgs;
  Args    = ;
  OptArgs = "," ident OptArgs;
  OptArgs = ;

  Expr  = Term ExprM;
  ExprM = "or" Term ExprM;
  ExprM = ;
  Term  = Factor TermM;
  TermM = "and" Factor TermM;
  TermM = ;
  Factor = "false";
  Factor = "true";
  Factor = ident;
  Factor = "not" Factor;
  Factor = "(" Expr ")";
}